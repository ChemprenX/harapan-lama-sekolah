{
    "name": "bantenprov/harapan-lama-sekolah",
    "description": "Harapan Lama Sekolah (HLS) wilayah Banten",
    "type": "library",
    "homepage": "https://github.com/bantenprov/harapan-lama-sekolah",
    "authors": [{
        "name": "bantenprov",
        "email": "developer.bantenprov@gmail.com",
        "role": "developer"
    }],
    "license": "MIT",
    "support": {
      "source": "https://github.com/bantenprov/harapan-lama-sekolah,",
      "issues": "https://github.com/bantenprov/harapan-lama-sekolah/issues",
      "wiki": "https://github.com/bantenprov/harapan-lama-sekolah/wiki"
    },
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.4.*",
        "laravel/tinker": "~1.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "files": [
            "src/helpers/helpers.php"
        ],
        "psr-4": {
            "Bantenprov\\HlSekolah\\": "src"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev"
}
